<?xml version="1.0" encoding="UTF-8"?>
<suite name="LiveGuru" parallel="false">

	<!-- this parameter url config url for all <test></test> -->
	
	<parameter name="url" value="https://blueimp.github.io/jQuery-File-Upload/" />

	<test name="Run On Firefox">
		<parameter name="browser" value="firefox" />
		<!-- <parameter name = "url" value = "http://live.demoguru99.com/"/> this 
			parameter config url one test -->

		<classes>
			<class
				name="com.jquery.uploadfile.Level_13_Handle_UploadFiles" />

		</classes>
	</test>

	<test name="Run On Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class
				name="com.jquery.uploadfile.Level_13_Handle_UploadFiles" />
		</classes>
	</test>

	<test name="Run On Edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class
				name="com.jquery.uploadfile.Level_13_Handle_UploadFiles" />
		</classes>
	</test>
<!--  -->

	<test name="Run On Coc Coc">
		<parameter name="browser" value="coccoc" />
		<parameter name="urlTargetCoccoc" value="C:\Program Files (x86)\CocCoc\Browser\Application\browser.exe" 
			/>
		<classes>
			<class
				name="com.jquery.uploadfile.Level_13_Handle_UploadFiles" />
		</classes>
	</test>
</suite> 
